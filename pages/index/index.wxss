/**index.wxss**/
.map{ flex:1; height: 100%; }
.map #map{ border: none}
.list-header{background-color:#ffffff; height: 100rpx;border-bottom: 1px solid #eee;position:fixed; top: -2rpx; left: 0rpx; width: 100%;z-index: 9999;box-sizing: border-box;  }
.header{position: absolute; top: 20rpx;background-color:#ffffff; border-radius: 20rpx; padding: 10rpx; border: 1px solid #ccc; }
.header cover-image,.header image{ width: 40rpx; height: 40rpx;display:block;line-height:1.2;
}
.header-center{ left: 50%; margin-left: -120rpx;}
.header-center{ width: 220rpx; text-align: center;border:none;border-radius:0rpx;}
.header-right{right: 20rpx;}
.header-right-2{right: 105rpx;}
.header-left{ left: 20rpx;}
.bottom{ position: absolute;  bottom: 20rpx; right: 10rpx;  background-color:#ffffff; border-radius: 30rpx; padding: 30rpx; border:none;   }
.bottom .bottom-count{ margin-top: 15rpx;}
.bottom .bottom-btn{ background: #000000; color: #ffffff; padding: 15rpx 0;border-radius: 30rpx; margin-left: 25rpx; width: 180rpx; text-align: center; font-size: 24rpx;}

.bottom-left{position: absolute;  bottom: 40rpx; left: 10rpx;background-color:#ffffff; border-radius: 20rpx; border: 1px solid #ccc;  padding: 5rpx;  }

.bottom-left cover-image{ width: 60rpx; height: 60rpx;}

.list-content{ position: relative; }

.list-view .list-bottom{ bottom: -2rpx; width: 100%; left: 0; border-radius: 0rpx; height: 100rpx; position:fixed; line-height: 100rpx; background-color: #fff;  box-shadow: 0px -2px 4px rgba(0, 0, 0, 0.1); }
.list-view .list-bottom .list-bottom-count{ margin-right: 14rpx}
.list-view{ flex: 1; height: 100%; overflow-y: hidden }
.list-bottom-btn{ background-color: #000; color: #fff; float: right; width: 220rpx; text-align: center}



